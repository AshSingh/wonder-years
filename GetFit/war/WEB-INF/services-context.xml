<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- services -->
	<bean id="usersServices"
	class="com.cs410.getfit.server.users.UsersServicesImpl">
		<property name="userDao" ref="userDao" />
	</bean>
	<bean id="challengesService"
	class="com.cs410.getfit.server.challenges.ChallengesServicesImpl">
		<property name="challengeDao" ref="challengeDao" />
		<property name="connectionSource" ref="connectionSource" />
	</bean>
	<bean id="challengeService"
	class="com.cs410.getfit.server.challenges.ChallengeServicesImpl">
		<property name="challengeDao" ref="challengeDao" />
	</bean>
</beans>